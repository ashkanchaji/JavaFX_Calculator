            </Button>
            <Button fx:id="multiply" focusTraversable="false" mnemonicParsing="false" onAction="#on_multiply_clicked" prefHeight="70.0" prefWidth="99.0" text="*" GridPane.columnIndex="3" GridPane.rowIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="subtract" focusTraversable="false" mnemonicParsing="false" onAction="#on_subtract_clicked" prefHeight="77.0" prefWidth="96.0" text="_" GridPane.columnIndex="3" GridPane.rowIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="addition" focusTraversable="false" mnemonicParsing="false" onAction="#on_addition_clicked" prefHeight="70.0" prefWidth="99.0" text="+" GridPane.columnIndex="3" GridPane.rowIndex="3">
