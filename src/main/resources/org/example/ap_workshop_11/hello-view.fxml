<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ap_workshop_11.CalculatorController">
   <children>
    <TextField fx:id="editor" editable="false" focusTraversable="false" prefHeight="55.0" prefWidth="411.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
    <GridPane hgap="10.0" prefHeight="282.0" prefWidth="401.0" vgap="10.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
        <Button fx:id="one" focusTraversable="false" mnemonicParsing="false" onAction="#on_one_clicked"  prefHeight="70.0" prefWidth="99.0" text="1" GridPane.rowIndex="2">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="five" focusTraversable="false" mnemonicParsing="false" onAction="#on_five_clicked"  prefHeight="70.0" prefWidth="99.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="two" focusTraversable="false" mnemonicParsing="false" onAction="#on_two_clicked"  prefHeight="70.0" prefWidth="99.0" text="2" GridPane.columnIndex="1" GridPane.rowIndex="2">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="four" focusTraversable="false" mnemonicParsing="false" onAction="#on_four_clicked" prefHeight="70.0" prefWidth="99.0" text="4" GridPane.rowIndex="1">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="seven" focusTraversable="false" mnemonicParsing="false" onAction="#on_seven_clicked"  prefHeight="70.0" prefWidth="99.0" text="7">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="eight" focusTraversable="false" mnemonicParsing="false" onAction="#on_eight_clicked"  prefHeight="70.0" prefWidth="99.0" text="8" GridPane.columnIndex="1">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="three" focusTraversable="false" mnemonicParsing="false" onAction="#on_three_clicked"  prefHeight="70.0" prefWidth="99.0" text="3" GridPane.columnIndex="2" GridPane.rowIndex="2">
          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="six" focusTraversable="false" mnemonicParsing="false" onAction="#on_six_clicked"  prefHeight="70.0" prefWidth="99.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="1">
