        </Button>
        <Button fx:id="equal" focusTraversable="false" mnemonicParsing="false" onAction="#on_equal_clicked" prefHeight="70.0" prefWidth="99.0" text="=" GridPane.columnIndex="2" GridPane.rowIndex="3">
