          <font>
            <Font size="20.0" />
          </font>
        </Button>
        <Button fx:id="decimal" focusTraversable="false" mnemonicParsing="false" onAction="#on_decimal_clicked" prefHeight="70.0" prefWidth="99.0" text="." GridPane.columnIndex="1" GridPane.rowIndex="3">
